#ifndef GK_GKVERSION_H
#define GK_GKVERSION_H

/**
 * The major version of GameKernel.
 */
#define GK_VERSION_MAJOR @VERSION_MAJOR@

/**
 * The minor version of GameKernel.
 */
#define GK_VERSION_MINOR @VERSION_MINOR@

/**
 * The patch version of GameKernel.
 */
#define GK_VERSION_PATCH @VERSION_PATCH@

/**
 * The version of GameKernel in the form of MAJOR.MINOR.PATCH
 */
#define GK_VERSION "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@"

/**
 * The build tag for this build of GameKernel based on the date. This is in the
 * form of YYYYMMDD_HHMMSS
 */
#define GK_VERSION_BUILD "@VERSION_BUILD@"

#endif
